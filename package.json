{
  "name": "multimediaobject",
  "version": "2.0.0",
  "description": "Multimediaobject library",
  "main": "index.js",
  "scripts": {
    "watch": "NODE_ENV=production webpack --watch --config config/webpack.config.js",
    "build": "webpack --config config/webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.js",
    "test:precompile": "webpack --config config/webpack.config.test.js",
    "test": "nyc ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://B-Core@bitbucket.org/B-Core/multimediaobject.git"
  },
  "keywords": [
    "MO",
    "MultimediaObject",
    "javascript"
  ],
  "sideEffects": false,
  "author": "Ciro DE CARO",
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/B-Core/multimediaobject#readme",
  "devDependencies": {
    "ava": "1.0.0-beta.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.5",
    "html-webpack-plugin": "^3.2.0",
    "nyc": "^11.8.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "joi": "^13.3.0"
  },
  "ava": {
    "files": [
      "tests/**/*.spec.js"
    ],
    "sources": [
      "src/MultimediaObject.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "compileEnhancements": false,
    "babel": false
  }
}
